<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.arg.arsoft.siantluis.domains.claim.ClaimUpload" table="tbl_claim_upload">
        <id name="id" column="id" type="int">
            <generator class="native"></generator>
        </id>
        <version name="version" column="version"></version>
        <property name="code" column="code"></property>
        <property name="desc" column="claim_desc"></property>
        <property name="claimDate" column="claim_date"></property>
        <property name="claimTime" column="claim_time"></property>
        <property name="claimLocation" column="claim_location"></property>
        <property name="claimReason" column="claim_reason"></property>
        <property name="createBy" column="create_by"></property>
        <property name="createDate" column="create_date"></property>
        <property name="lastupdateBy" column="lastupdate_by"></property>
        <property name="lastupdateDate" column="lastupdate_date"></property>
    </class>
  

</hibernate-mapping>
