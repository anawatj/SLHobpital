<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.arg.arsoft.siantluis.domains.attachment.AttachmentItem" table="tbl_attachment_item">
        <id name="id" column="id" type="int">
            <generator class="native"></generator>
        </id>
        <version name="version" column="version"></version>
        <property name="attachmentId" column="attachment_id"></property>
        <property name="name" column="name"></property>
        <many-to-one name="type" column="attachment_type" class="com.arg.arsoft.siantluis.domains.master.AttachmentType"></many-to-one>
        <property name="fileName" column="attachment_file"></property>
        <property name="createBy" column="create_by"></property>
        <property name="createDate" column="create_date"></property>
        <property name="lastupdateBy" column="lastupdate_by"></property>
        <property name="lastupdateDate" column="lastupdate_date"></property>
    </class>
  

</hibernate-mapping>
