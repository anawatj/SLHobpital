<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.arg.arsoft.siantluis.domains.project.ProjectItem" table="tbl_project_item">
        
        <id name="id" column="id" type="int">
            <generator class="native"></generator>
        </id>
        <version name="version" column="version"></version>
        <property name="projectGroup" column="item_desc"></property>
        <property name="projectId" column="project_id"></property>
        <set name="frms" cascade="all-delete-orphan">
            
            <key column="project_item_id"></key>
            <one-to-many class="com.arg.arsoft.siantluis.domains.project.ProjectItemFrm" not-found="ignore"></one-to-many>
        </set>
        <property name="createBy" column="create_by"></property>
        <property name="createDate" column="create_date"></property>
        <property name="lastupdateBy" column="lastupdate_by"></property>
        <property name="lastupdateDate" column="lastupdate_date"></property>
    </class>
  

</hibernate-mapping>
